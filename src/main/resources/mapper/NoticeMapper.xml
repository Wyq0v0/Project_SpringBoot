<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wyq.project_springboot.mapper.NoticeMapper">
    <insert id="insertNotice"  useGeneratedKeys="true" keyProperty="id">
        insert into `t_notice` (`admin_id`,`title`,`content`,`type`) values(#{adminId},#{title},#{content},#{type.value})
    </insert>
</mapper>