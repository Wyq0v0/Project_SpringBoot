<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wyq.project_springboot.mapper.GoodsCategoryMapper">

    <select id="selectGoodsCategory" resultType="com.wyq.project_springboot.entity.GoodsCategory">
        select * from `t_goods_category` where `id` = #{categoryId}
    </select>
    <select id="selectGoodsCategoryList" resultType="com.wyq.project_springboot.entity.GoodsCategory">
        select * from `t_goods_category`
    </select>
</mapper>